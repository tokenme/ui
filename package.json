{
  "name": "Tokenme-UI",
  "version": "0.0.1",
  "description": "Tokenme UI based on Vue.js 2.0",
  "author": "prof.syd.xu@gmail.com",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server -d --inline --hot --env.dev",
    "build": "npm run copy && npm run precache && rimraf build/dist && webpack --config webpack.config.js -p --progress --hide-modules && cp -r build/sw/* build/dist/ && cp -r build/assets/* build/dist/",
    "build-dev": "npm run copy && npm run precache && rimraf build/dev && webpack --config webpack.dev.config.js -p --progress --hide-modules && cp -r build/sw/* build/dev/ && cp -r build/assets/* build/dev/",
    "copy": "rimraf build/sw && mkdir build/sw && cp node_modules/sw-toolbox/sw-toolbox.js build/sw/sw-toolbox.js",
    "precache": "./node_modules/sw-precache/cli.js --root=build/sw --config=sw-precache-config.json",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "vue": "^2.5.2",
    "vuetify": "^1.0.1",
    "weixin-js-sdk": "^1.3.3"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue-app": "^1.2.0",
    "css-loader": "^0.28.4",
    "sass-loader": "^6.0.6",
    "precss": "^2.0.0",
    "cssnano": "^3.10.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "less": "^2.7.3",
    "less-loader": "^2.2.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "eslint": "^4.4.0",
    "eslint-config-enough": "^0.2.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.0.0",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.30.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.0",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.4",
    "vue-meta-info": "^0.1.1",
    "vue-social-sharing": "^2.3.3",
    "vue-i18n": "^7.4.2",
    "vuex": "^2.3.0",
    "v-qrcode": "^1.0.3",
    "axios": "^0.21.1",
    "vue-clipboard2": "^v0.0.8",
    "chart.js": "^2.7.1",
    "vue-chartjs": "^3.2.1",
    "vue-gtagjs": "^1.0.7",
    "vue-gtagjs-directive": "^1.0.3",
    "moment": "^2.20.1",
    "webpack": "^3.4.1",
    "preload-webpack-plugin": "^2.3.0",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "webpack-dev-server": "^3.11.2",
    "sw-precache": "^5.2.1",
    "sw-toolbox": "^3.6.0",
    "node-sass": "^4.5.3"
  }
}
